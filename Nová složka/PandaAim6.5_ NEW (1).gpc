function WII_HOME(x, y, size, color) {
	ps4[Buffer]--;
	
	if(x == AlignRight) {
		x = OLED_WIDTH - (ps4[Buffer] * byte[size]) - MK_AXIS_Y;
	} else if(x == AlignCenter) {
		x = (OLED_WIDTH >> MK_ADS_GAIN_RATIO) - ((ps4[Buffer] * byte[size]) >> POLAR_ANGLE);
	}
	
	if(y == AlignBottom) {
		y = OLED_HEIGHT - char[size] - MK_AXIS_X;
	} else if(y == AlignCenter) {
		y = (OLED_HEIGHT >> POLAR_ANGLE) - (char[size] >> MK_ADS_GAIN_RATIO);
	}
	
	puts_oled(clamp(x, MK_SENSITIVITY, ASCII_PS_CROSS), clamp(y, MK_SENSITIVITY, ASCII_QUESTION), size, ps4[Buffer], color);
	ps4[Buffer] = MK_GENERAL;	
}

