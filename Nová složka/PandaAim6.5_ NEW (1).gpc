function DFGT_SELECT(do, while, for, continue, if) {
	if(continue > DF_PS) {
		if(do + continue <= for) {
			XB1_RB(MenuGreen);
			return do + continue;
		} else if(if) {
			XB1_RB(MenuGreen);
			return while;
		}
	} else {
		if(do + continue >= while) {
			XB1_RB(MenuGreen);
			return do + continue;
		} else if(if) {
			XB1_RB(MenuGreen);
			return for;
		}
	}

	if(!WII_IRX(WII_IRX(continue > DFGT_PS, do == for, do == while), FALSE, TRUE)) {
		XB1_RB(MenuRed);
	}

	return WII_IRX(continue > DFGT_PS, for, while);
}

