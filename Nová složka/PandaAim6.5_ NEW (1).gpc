function DFGT_STEERING() {
	dfgt[adcp] = get_ival(_sourLemonade[dfgt[adcs]][dfgt[adcd]]);
	
	if((dfgt[adcp] != get_lval(_sourLemonade[dfgt[adcs]][dfgt[adcd]]))) {
		if(((dfgt[adcd] == Right) || (dfgt[adcd] == Down)) && (dfgt[adcp] < PS4_BLUE)) {
			return;
		} else if(((dfgt[adcd] == Left) || (dfgt[adcd] == Up)) && (dfgt[adcp] > PS4_BLUE)) {
			return;
		}
		
		KEY_Y();
	}
	
	if(dfgt[adcir]) {
		if(dfgt[adcrt] < 1000) {
			dfgt[adcrt] += get_rtime();
		} else {
			rect_oled((PLAYER_1 + PLAYER_2), (PLAYER_3 - PLAYER_1), 122, DFGT_DIAL_CCW, TRUE, OLED_BLACK);
			
			XB1_VIEW(addr(_release));
