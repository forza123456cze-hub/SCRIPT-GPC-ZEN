			if(xbox[dc] == PIO_XB1) {
				XB1_VIEW(WII_LT(((BITMASK_3 << 2) + BITMASK_2 + BITMASK_1)));
			} else {
				XB1_XBOX(OLED_CROSS);
			}
			
			WII_HOME(AlignCenter, DFGT_CIRCLE, OLED_FONT_SMALL, OLED_WHITE);
			
			dfgt[adccs] = (DFGT_DIAL / DFGT_DIAL); 
			KEY_5(MenuGreen);
		}
	} else {
		dfgt[adcrt] = DFGT_PS;
	}
	
	dfgt[adcp] = abs(dfgt[adcp]);
	
	KEY_ESC((dfgt[adcp] >= DFGT_DIAL_CW) && (dfgt[adcp] <= XB1_PR2));
}

function DFGT_BRAKE(byte, string) {
	if(get_ival(byte)) {
		if(nintendo[ButtonBlock] == -1)  {
			nintendo[ButtonBlock] = byte;
		}
		
		if(nintendo[ButtonBlock] == byte) {
			if(event_press(byte)) {
				return TRUE;
			}
			
			if(string) {
                if (get_ptime(byte) >= (KEY_102ND * XB360_START)) {
                    if (get_ptime(byte) > 4000)  {
                        nintendo[ButtonHoldTime] = DFGT_START(G25_CLUTCH);
                    } else if (get_ptime(byte) > 3000) {
                        nintendo[ButtonHoldTime] = DFGT_START(G25_CLUTCH + G25_SQUARE);
                    } else if (get_ptime(byte) > 2000) {
                        nintendo[ButtonHoldTime] = DFGT_START(KEY_HASHTILDE);
                    } else if (get_ptime(byte) > 1000) {
                        nintendo[ButtonHoldTime] = DFGT_START(KEY_SYSRQ);
                    } else {
                        nintendo[ButtonHoldTime] = DFGT_START(KEY_KP2);
                    }
                    
                } else {
                	return FALSE;
                }
			} else {
				nintendo[ButtonHoldTime] = WII_IRX(nintendo[ButtonHoldTime] >= (KEY_102ND << XB360_BACK), (KEY_B - KEY_B), nintendo[ButtonHoldTime] + KEY_G);
			}
			
			return !nintendo[ButtonHoldTime];
		}
	}
	else if(event_release(nintendo[ButtonBlock])) {
		nintendo[ButtonBlock] = -1;	
		nintendo[ButtonHoldTime] = (DF_SQUARE - XB360_X);
		KEY_Y();
	}
	
	return FALSE;
}

function DFGT_UP(char) {
	return event_release(char) && (get_ptime(char) < (KEY_102ND * XB360_RY));
}

function DFGT_DOWN(uint8, uint16, ps5adt) {
	KEY_MINUS();
	
	if(DFGT_BRAKE(((BITMASK_6 >> 1) - PS5_ADT_FORCE2), WII_IRX(ps5adt || (swi[Screen] == AntiRecoilMenu), TRUE, FALSE))) {
		xb360[nintendo[EditIndex]] = DFGT_SELECT(
			xb360[nintendo[EditIndex]],
			uint8,
			uint16,
			1,
			!ps5adt
		);
		
		return TRUE; 
		
	} else if(DFGT_BRAKE(((BITMASK_4 + BITMASK_3 + BITMASK_2 - BITMASK_1 + sizeof(char))), WII_IRX(ps5adt || (swi[Screen] == AntiRecoilMenu), TRUE, FALSE))) {
		xb360[nintendo[EditIndex]] = DFGT_SELECT(
			xb360[nintendo[EditIndex]],
			uint8,
			uint16,
			-1,
			!ps5adt
		);
		
		return TRUE;
	}
	
	return FALSE;
}

