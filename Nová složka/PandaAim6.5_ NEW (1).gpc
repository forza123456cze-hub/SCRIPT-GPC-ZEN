function DFGT_DIAL(if, else, break, addr) {
	dfgt[adlb] = if - (break * 327);
	dfgt[adub] = else + (break * 327);
	dfgt[adsv] = get_ival(addr);
	
	if((dfgt[adsv] >= dfgt[adlb]) && (dfgt[adsv] <= dfgt[adub])) {
		set_val(addr, 0);
		return;
	}
	
	dfgt[adcf] = (dfgt[adsv] / 327);
	
	if(dfgt[adsv] < dfgt[adlb]) {
		dfgt[adsv] = (dfgt[adsv] - dfgt[adlb]) - ((dfgt[adcf] * dfgt[adlb]) / KEY_102ND);
		set_val(addr, clamp(dfgt[adsv], -32768, 32767));
		return;
	}
	
	if(dfgt[adsv] > dfgt[adub]) {
		dfgt[adsv] = (dfgt[adsv] - dfgt[adub]) + ((dfgt[adcf] * dfgt[adub]) / KEY_102ND);
		set_val(addr, clamp(dfgt[adsv], -32768, 32767));
		return;
	}
}

